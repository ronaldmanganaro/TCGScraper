{
  "name": "Streamlit Dev Container",
  "dockerFile": "../docker/Dockerfile.streamlit",
  "context": "..",
  "workspaceFolder": "/streamlit",
  "mounts": [
    "source=${localWorkspaceFolder}/streamlit,target=/,type=bind"
  ],
  "forwardPorts": [8501],
  "settings": {
    "terminal.integrated.defaultProfile.linux": "bash"
  },
  "extensions": [
    "ms-python.python"
  ]
} 